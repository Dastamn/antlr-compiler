        c := 3;

        x := 5 + 3;

        // x := a * ((a * b + c) - (c * -b));

    	Si (!(5 + 1 > -3 & 5 < 2 | 10 = 7)) Alors {
            x := 5;
            Si (5 < 2) Alors {
                x := 5;
            } Sinon {
                x := 1 + 2;
