import Small_Java.lang;
import Small_Java.io;

class_SJ Test {
	int_SJ  x, a;
    main_SJ {
    	x := 0;
    	Si (x = 0) Alors {
    	    x := x + 1;
    	} Sinon {
    	    Si (1 > 2) Alors {
                x := x + 10;
    	    } Sinon Si (1 = 1) Alors {
    	        x := x + 5;
    	    }
    	}
    }
}